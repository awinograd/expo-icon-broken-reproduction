      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^@src/types(.*)$": "<rootDir>/noop.ts",
      "^@src(.*)$": "<rootDir>/src$1"
    },
    "preset": "react-native",
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "/__tests__/*",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
